{
  "name": "stringbot",
  "version": "0.0.1",
  "description": "A bot for managing everything in your server.",
  "main": ".build/index.js",
  "types": "src/index.d.ts",
  "scripts": {
    "start": "node .build/index.js",
    "dev": "tsc && node .build/index.js",
    "dev:clean": "rm -rf .build && tsc && node .build/index.js",
    "lint": " yarn eslint src",
    "lint:fix": "yarn eslint src --fix",
    "deploy": "tsc src/deploy-commands.ts --target ES6 --module CommonJs && node src/deploy-commands.js && rm -rf src/deploy-commands.js",
    "build": "rm -rf .build && tsc"
  },
  "author": "@Megatank58",
  "license": "MIT",
  "dependencies": {
    "@discordjs/rest": "^0.1.0-canary.0",
    "discord-api-types": "^0.23.1",
    "discord.js": "^13.2.0",
    "dotenv": "^10.0.0"
  },
  "devDependencies": {
    "@types/node": "^16.10.3",
    "@types/node-fetch": "2.5.12",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.4.1",
    "typescript": "^4.4.3"
  }
}
